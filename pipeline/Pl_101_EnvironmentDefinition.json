{
	"name": "Pl_101_EnvironmentDefinition",
	"properties": {
		"activities": [
			{
				"name": "Environment Variables",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "Ds_ControlDb",
						"type": "DatasetReference",
						"parameters": {
							"ServerName": {
								"value": "@concat(\n     'siaf'\n    ,pipeline().globalParameters.Gp_Environment\n    ,pipeline().globalParameters.Gp_CustName\n    ,'sqlserver.database.windows.net'\n\n)",
								"type": "Expression"
							},
							"DatabaseName": {
								"value": "@concat(\n     'siaf'\n    ,pipeline().globalParameters.Gp_Environment\n    ,pipeline().globalParameters.Gp_CustName\n    ,'sqldatabase'\n\n)",
								"type": "Expression"
							},
							"TableSchema": "ztg",
							"TableName": "MyTable"
						}
					}
				}
			}
		],
		"variables": {
			"Va_SqlServerName": {
				"type": "String"
			}
		},
		"folder": {
			"name": "100_Control"
		},
		"annotations": []
	}
}