{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "siafdevcustnamedatafactory"
		},
		"ls_AzDataLakeStorage_SAMI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().pa_stAccUrl}"
		},
		"ls_ControlDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().pa_serverName}"
		},
		"ls_ControlDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().pa_databaseName}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "siafdevcustnamedatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzDataLakeStorage_SAMI_properties_typeProperties_url": {
						"value": "[parameters('ls_AzDataLakeStorage_SAMI_properties_typeProperties_url')]"
					},
					"ls_ControlDb_properties_typeProperties_server": {
						"value": "[parameters('ls_ControlDb_properties_typeProperties_server')]"
					},
					"ls_ControlDb_properties_typeProperties_database": {
						"value": "[parameters('ls_ControlDb_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}